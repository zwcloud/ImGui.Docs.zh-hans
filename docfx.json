{
    "metadata": [
        {
            "src": [
                {
                    "files": [
                        "project.json"
                    ],
                    "exclude": [
                        "**/obj/**",
                        "**/bin/**",
                        "_site/**"
                    ],
                    "src": "../../ImGui/ImGui/"
                }
            ],
            "dest": "api"
        }
    ],
    "build": {
        "content": [
            {
                "files": [
                    "api/**.yml",
                    "api/index.md"
                ]
            },
            {
                "files": [
                    "articles/**.md",
					"articles/**/toc.yml",
                    "toc.yml",
                    "*.md"
                ],
                "exclude": [
                    "obj/**",
                    "_site/**"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**",
                    "**/*.png",
                    "**/*.svg",
                    "**/*.jpg",
                    "**/*.gif"
                ],
                "exclude": [
                    "obj/**"
                ]
            }
        ],
        "overwrite": [
            {
                "files": [
                    "apidoc/**.md"
                ],
                "exclude": [
                    "obj/**"
                ]
            }
        ],
        "dest": "../gh-pages",
        "globalMetadata": {
	        "_appTitle" : "ImGui Doc",
			"_appLogoPath" : "images/imgui_logo.svg",
			"_appFaviconPath" : "images/imgui_favico.png",
		    "_appFooter" : "<span>Copyright Â© 2016 zwcloud<br>Generated by <strong>DocFX</strong></span>"
		},
        "fileMetadataFiles": [],
        "template": [
            "default",
			"template"
        ],
        "postProcessors": [],
        "noLangKeyword": false
    }
}